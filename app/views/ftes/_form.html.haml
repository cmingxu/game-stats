= form_for(@fte) do |f|
  - if @fte.errors.any?
    #error_explanation
      %h2
        = pluralize(@fte.errors.count, "error")
        prohibited this fte from being saved:
      %ul
        - @fte.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :player_id
    %br/
    = f.number_field :player_id
  .field
    = f.label :step
    %br/
    = f.number_field :step
  .field
    = f.label :pass_time
    %br/
    = f.datetime_select :pass_time
  .field
    = f.label :app_id
    %br/
    = f.number_field :app_id
  .actions
    = f.submit
